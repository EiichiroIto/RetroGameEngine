{ #category : #accessing }
Sample02 class >> title [
	^ '02 Jump Game'
]

{ #category : #api }
Sample02 >> draw [
	| offset s |
	engine cls: 12.
	engine
		blt: 0 @ 88
		imageBank: 0
		at: 0 @ 88
		extent: 160 @ 32
		color: nil.
	engine
		blt: 0 @ 88
		imageBank: 0
		at: 0 @ 64
		extent: 160 @ 24
		color: 12.
	offset := engine frameCount \\ 160.
	0 to: 1 do: [ :each | 
		engine
			blt: (each * 160 - offset) @ 104
			imageBank: 0
			at: 0 @ 48
			extent: 160 @ 16
			color: 12 ].
	offset := engine frameCount // 16 \\ 160.
	0 to: 1 do: [ :i | 
		farCloud
			do: [ :each | 
				engine
					blt: (each x + (i * 160) - offset) @ each y
					imageBank: 0
					at: 64 @ 32
					extent: 32 @ 8
					color: 12 ] ].
	offset := engine frameCount // 8 \\ 160.
	0 to: 1 do: [ :i | 
		nearCloud
			do: [ :each | 
				engine
					blt: (each x + (i * 160) - offset) @ each y
					imageBank: 0
					at: 0 @ 32
					extent: 56 @ 8
					color: 12 ] ].
	floor
		do: [ :each | 
			engine
				blt: each first @ each second
				imageBank: 0
				at: 0 @ 16
				extent: 40 @ 8
				color: 12 ].
	fruit
		do: [ :each | 
			each fourth
				ifTrue: [ engine
						blt: each first @ each second
						imageBank: 0
						at: (32 + (each third * 16)) @ 0
						extent: 16 @ 16
						color: 12 ] ].
	engine
		blt: playerPosition
		imageBank: 0
		at:
			(playerDy > 0
				ifTrue: [ 16 ]
				ifFalse: [ 0 ]) @ 0
		extent: 16 @ 16
		color: 12.
	s := 'SCORE ' , score asString.
	engine text: s at: 5 @ 4 color: 1.
	engine text: s at: 4 @ 4 color: 7
]

{ #category : #api }
Sample02 >> init [
	engine
		extent: 160 @ 120;
		title: 'Jump Game';
		init.
	(engine imageBank: 0) importString: self class imageBank0 at: 0 @ 0.
	score := 0.
	playerPosition := 72 @ -16.
	playerDy := 0.
	isAlive := true.
	farCloud := {(-10 @ 75).
	(40 @ 65).
	(90 @ 60)}.
	nearCloud := {(10 @ 25).
	(70 @ 35).
	(120 @ 15)}.
	floor := (0 to: 3)
		collect: [ :each | 
			{(each * 60).
			(engine randomFrom: 8 to: 104).
			true} ].
	fruit := (0 to: 3)
		collect: [ :each | 
			{(each * 60).
			(engine randomFrom: 0 to: 104).
			(engine randomFrom: 0 to: 2).
			true} ]
]

{ #category : #api }
Sample02 >> update [
	(engine isPressing: (RetroGameInputs key: $Q))
		ifTrue: [ self quit ].
	self updatePlayer.
	self updateFloor.
	self updateFruit
]

{ #category : #private }
Sample02 >> updateFloor [
	floor := floor
		collect: [ :each | 
			| x y alive |
			x := each first.
			y := each second.
			alive := each third.
			alive
				ifTrue: [ (playerPosition x + 16 >= x
						and: [ (playerPosition x <= (x + 40))
								and: [ playerPosition y + 16 >= y
										and: [ (playerPosition y <= (y + 8)) and: [ playerDy > 0 ] ] ] ])
						ifTrue: [ alive := false.
							score := score + 10.
							playerDy := -12 ] ]
				ifFalse: [ y := y + 6 ].
			x := x - 4.
			x < -40
				ifTrue: [ x := x + 240.
					y := engine randomFrom: 8 to: 104.
					alive := true ].
			Array with: x with: y with: alive ]
]

{ #category : #private }
Sample02 >> updateFruit [
	fruit := fruit
		collect: [ :each | 
			| x y kind alive |
			x := each first.
			y := each second.
			kind := each third.
			alive := each fourth.
			(alive
				and: [ (x - playerPosition x) abs < 12
						and: [ (y - playerPosition y) abs < 12 ] ])
				ifTrue: [ alive := false.
					score := score + ((kind + 1) * 100).
					playerDy := playerDy min: -8 ].
			x := x - 2.
			x < -40
				ifTrue: [ x := x + 240.
					y := engine randomFrom: 0 to: 104.
					kind := engine randomFrom: 0 to: 2.
					alive := true ].
			Array
				with: x
				with: y
				with: kind
				with: alive ]
]

{ #category : #private }
Sample02 >> updatePlayer [
	| x |
	x := playerPosition x.
	(engine isPressing: RetroGameInputs left)
		ifTrue: [ x := x - 2 max: 0 ].
	(engine isPressing: RetroGameInputs right)
		ifTrue: [ x := x + 2 min: engine width - 16 ].
	playerPosition := x @ (playerPosition y + playerDy).
	playerDy := playerDy + 1 min: 8.
	playerPosition y > engine height
		ifFalse: [ ^ self ].
	isAlive
		ifTrue: [ isAlive := false ].
	playerPosition y > 600
		ifFalse: [ ^ self ].
	score := 0.
	playerPosition := 72 @ -16.
	playerDy := 0.
	isAlive := true
]
