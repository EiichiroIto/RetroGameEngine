	^ '






























'
]

{ #category : #constants }
Sample10 class >> tileFloor [
	^ 1 @ 0
]

{ #category : #constants }
Sample10 class >> tileSpawn1 [
	^ 0 @ 1
]

{ #category : #constants }
Sample10 class >> tileSpawn2 [
	^ 1 @ 1
]

{ #category : #constants }
Sample10 class >> tileSpawn3 [
	^ 2 @ 1
]

{ #category : #accessing }
Sample10 class >> title [
	^ '10 Platformer Game'
]

{ #category : #constants }
Sample10 class >> transparentColor [
	^ 2
]

{ #category : #constants }
Sample10 class >> wallTileX [
	^ 4
]

{ #category : #api }
Sample10 >> addEnemy: anEnemy [
	enemies add: anEnemy
]

{ #category : #private }
Sample10 >> detectCollision: aPoint with: anInteger [
	| x1 x2 y1 y2 |
	x1 := aPoint x // 8.
	y1 := aPoint y // 8.
	x2 := (aPoint x + 8 - 1) // 8.
	y2 := (aPoint y + 8 - 1) // 8.
	y1 to: y2 do: [ :yi | 
		x1 to: x2 do: [ :xi | 
			(self getTile: xi @ yi) x >= self class wallTileX
				ifTrue: [ ^ true ] ] ].
	(anInteger > 0 and: [ aPoint y \\ 8 = 1 ])
		ifTrue: [ x1 to: x2 do: [ :xi | 
				(self getTile: xi @ (y1 + 1)) = self class tileFloor
					ifTrue: [ ^ true ] ] ].
	^ false
]

{ #category : #api }
Sample10 >> draw [
	engine cls: 0.
	engine resetCamera.
	engine
		blt: 0 @ 0
		tileMap: 0
		at: (scrollX // 4 \\ 128) @ 128
		extent: 128 @ 128
		transparentColor: nil.
	engine
		blt: 0 @ 0
		tileMap: 0
		at: scrollX @ 0
		extent: 128 @ 128
		transparentColor: self class transparentColor.
	engine camera: scrollX @ 0.
	player draw.
	enemies do: #draw
]

{ #category : #accessing }
Sample10 >> enemies [
	^ enemies
]

{ #category : #accessing }
Sample10 >> enemies: anObject [
	enemies := anObject
]

{ #category : #private }
Sample10 >> gameOver [
	scrollX := 0.
	player position: 0 @ 0.
	player delta: 0 @ 0.
	enemies := OrderedCollection new.
	self spawnEnemy: 0 with: 127
]

{ #category : #private }
Sample10 >> getTile: aPoint [
	^ (engine tileMap: 0) pairAt: aPoint
]

{ #category : #api }
Sample10 >> init [
	engine
		extent: 128 @ 128;
		title: 'Platformer';
		init.
	(engine imageBank: 0) importString: self class image0 at: 0 @ 0.
	(engine tileMap: 0)
		importString: self class tile0 at: 0 @ 0;
		imageBank: 0.
	(engine imageBank: 0)
		rect: 0 @ 8
		extent: 24 @ 8
		color: self class transparentColor.
	player := self newPlayerAt: 0 @ 0.
	scrollX := 0.
	enemies := OrderedCollection new.
	self spawnEnemy: 0 with: 127
]

{ #category : #testing }
Sample10 >> isWall: aPoint [
	| tile |
	tile := self getTile: (aPoint x // 8) @ (aPoint y // 8).
	^ tile = self class tileFloor
		or: [ tile x >= self class wallTileX ]
]

{ #category : #private }
Sample10 >> newEnemy1At: aPoint [
	^ Sample10Enemy1 new
		position: aPoint;
		init: self;
		yourself
]

{ #category : #private }
Sample10 >> newEnemy2At: aPoint [
	^ Sample10Enemy2 new
		position: aPoint;
		init: self;
		yourself
]

{ #category : #private }
Sample10 >> newEnemy3At: aPoint [
	^ Sample10Enemy3 new
		position: aPoint;
		init: self;
		yourself
]

{ #category : #private }
Sample10 >> newPlayerAt: aPoint [
	^ Sample10Player new
		position: aPoint;
		init: self;
		yourself
]

{ #category : #accessing }
Sample10 >> player [
	^ player
]

{ #category : #accessing }
Sample10 >> player: anObject [
	player := anObject
]

{ #category : #private }
Sample10 >> pushBack1: aPoint delta: aPoint2 [
	| sign x |
	sign := aPoint2 x sign.
	x := aPoint x.
	aPoint2 x abs
		timesRepeat: [ (self detectCollision: (x + sign) @ aPoint y with: aPoint2 y)
				ifTrue: [ ^ x ].
			x := x + sign ].
	^ x
]

{ #category : #private }
Sample10 >> pushBack2: aPoint delta: aPoint2 [
	| sign y |
	sign := aPoint2 y sign.
	y := aPoint y.
	aPoint2 y abs
		timesRepeat: [ (self detectCollision: aPoint x @ (y  + sign) with: aPoint2 y)
				ifTrue: [ ^ y ].
			y := y + sign ].
	^ y
]

{ #category : #api }
Sample10 >> pushBack: aPoint delta: aPoint2 [
	| x y |
	x := aPoint x.
	y := aPoint y.
	aPoint2 x abs > aPoint2 y abs
		ifTrue: [ x := self pushBack1: x @ y delta: aPoint2.
			y := self pushBack2: x @ y delta: aPoint2 ]
		ifFalse: [ y := self pushBack2: x @ y delta: aPoint2.
			x := self pushBack1: x @ y delta: aPoint2 ].
	^ x @ y
]

{ #category : #accessing }
Sample10 >> scrollX [
	^ scrollX
]

{ #category : #accessing }
Sample10 >> scrollX: anObject [
	scrollX := anObject
]

{ #category : #api }
Sample10 >> spawnEnemy: anInteger with: anInteger2 [
	| leftX rightX tile |
	leftX := engine ceil: anInteger / 8.
	rightX := engine floor: anInteger2 / 8.
	leftX to: rightX do: [ :x | 
		0 to: 15 do: [ :y | 
			tile := self getTile: x @ y.
			tile = self class tileSpawn1
				ifTrue: [ enemies add: (self newEnemy1At: (x * 8) @ (y * 8)) ].
			tile = self class tileSpawn2
				ifTrue: [ enemies add: (self newEnemy2At: (x * 8) @ (y * 8)) ].
			tile = self class tileSpawn3
				ifTrue: [ enemies add: (self newEnemy3At: (x * 8) @ (y * 8)) ] ] ]
]

{ #category : #api }
Sample10 >> update [
	(engine isPressing: (RetroGameInputs key: $Q))
		ifTrue: [ self quit ].
	player update.
	enemies
		do: [ :each | 
			((player position x - each position x) abs < 6
				and: [ (player position y - each position y) abs < 6 ])
				ifTrue: [ ^ self gameOver ].
			each update.
			(each position x < (scrollX - 8)
				or: [ each position x > (scrollX + 160) or: [ each position y > 160 ] ])
				ifTrue: [ each isAlive: false ] ].
	enemies := enemies select: #isAlive
]
